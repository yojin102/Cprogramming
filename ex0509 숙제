// 키보드로부터 정수를 입력받아, 구구단(몇 단)을 출력
	printf("원하는 구구단의 단(1~9)을 입력하세요 ");
	int num;
	scanf_s("%d", &num);

	// for(초기식; 조건식; 증감식)
	for (int i = 1; i < 10; i++)
	{
		printf("%d * %d = %d\n", num, i, num*i);
	}
  
  // while문
  printf("원하는 구구단의 단(1~9)을 입력하세요 ");
	int num = 10;
	int i = 1;
	
		scanf_s("%d", &num);

	while (i < 10)
	{
		printf("%d * %d = %d\n", num, i, num * i);
		i++;
	}
